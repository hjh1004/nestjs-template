{
    "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-config-airbnb"],
    "rules": {
      "prettier": true,
      "quotemark": [true, "single"],
      "variable-name": {
        "options": [
          "ban-keywords",
          "check-format",
          "allow-leading-underscore",
          "allow-pascal-case",
          "allow-snake-case"
        ]
      },
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "never",
            "typeLiterals": "ignore"
          },
          "esSpecCompliant": true
        }
      ],
      "no-console": [true, "log", "error"]
    }
  }
  